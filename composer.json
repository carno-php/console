{
  "name": "carno-php/console",
  "license": "MIT",
  "require": {
    "carno-php/container": "^1.0",
    "carno-php/coroutine": "^1.0",
    "carno-php/promise": "^1.0",
    "carno-php/config": "^1.0",
    "carno-php/log-slim": "^1.0",
    "symfony/var-dumper": "~3.3|~4.0",
    "symfony/console": "~3.3|~4.0"
  },
  "autoload": {
    "psr-4": {
      "Carno\\Console\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Carno\\Console\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
